{
  "presets": [
    [
      "@babel/preset-env", 
      {
        "targets": [  // @babel/polyfill targeting browser that only the last 2 versions, not dead, not less than 2%
          "last 2 versions",
          "not dead",
          "not < 2%",
          "not ie 11"
        ],
        "useBuiltIns": "entry"
      }
    ], 
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel", 
    "@babel/plugin-proposal-class-properties", 
    "@babel/plugin-syntax-dynamic-import"
  ]
}